   "source": [
    "def myindexes(txt,char):\n",
    "    idxs=[]\n",
    "    idx=0\n",
    "    for item in txt:\n",
    "        if item == char:\n",
    "            idxs.append(idx)\n",
    "        idx+=1\n",
    "    return idxs"
   ]
  },
